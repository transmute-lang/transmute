        * {
            font-family: monospace;
            color: #a9b7c6;
            cursor: default;
        }

        .line {
            position:absolute;
            left:0;
        }

        ul {
            padding: 0;
        }

        li {
            list-style: none;
            padding-left: 2em;
        }

        span {
            display: inline-block;
